<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.thfw.mobileheart">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.FOREGROUND_SERVICE" />

    <uses-feature android:name="android.hardware.camera2.full" />

    <queries>
        <intent>
            <action android:name="android.intent.action.TTS_SERVICE" />
        </intent>
    </queries>

    <application
        android:name="com.thfw.mobileheart.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.thfw.mobileheart.activity.robot.RobotDetailsActivity"></activity>
        <activity android:name="com.thfw.mobileheart.activity.robot.RobotActivity" />
        <activity android:name="com.thfw.mobileheart.activity.me.MeTestHistoryActivity" />
        <activity android:name="com.thfw.mobileheart.activity.me.MeHistoryActivity" />
        <activity android:name="com.thfw.mobileheart.activity.test.TestBeginActivity" />
        <activity android:name="com.thfw.mobileheart.activity.exercise.ExerciseIngActivity" />
        <activity android:name="com.thfw.mobileheart.activity.read.ReadDetailsActivity" />
        <activity android:name="com.thfw.mobileheart.activity.me.ServiceHotLineActivity" />
        <activity android:name="com.thfw.mobileheart.activity.me.HealthReportActivity" />
        <activity android:name="com.thfw.mobileheart.activity.me.AssessReportActivity" />
        <activity android:name="com.thfw.mobileheart.activity.settings.MeAskListActivity" />
        <activity android:name="com.thfw.mobileheart.activity.settings.BindMobileActivity" />
        <activity android:name="com.thfw.mobileheart.activity.settings.AccountSafeActivity" />
        <activity android:name="com.thfw.mobileheart.activity.settings.MeWillHelpBackActivity" />
        <activity android:name="com.thfw.mobileheart.activity.settings.HelpHintsActivity" />
        <activity android:name="com.thfw.mobileheart.activity.settings.HelpBackActivity" />
        <activity android:name="com.thfw.mobileheart.activity.audio.AudioEtcActivity" />
        <activity android:name="com.thfw.mobileheart.activity.exercise.ExerciseDetailActivity" />
        <activity android:name="com.thfw.mobileheart.activity.me.CollectActivity" />
        <activity android:name="com.thfw.mobileheart.activity.integral.ClockInActivity" />
        <activity android:name="com.thfw.mobileheart.activity.test.TestProgressIngActivity" />
        <activity
            android:name="com.thfw.mobileheart.activity.ScrollingActivity"
            android:label="@string/title_activity_scrolling"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.thfw.mobileheart.activity.heartbox.MeHeartBoxActivity" />
        <activity android:name="com.thfw.mobileheart.activity.heartbox.HeartBoxActivity" />
        <activity android:name="com.thfw.mobileheart.activity.settings.InfoActivity" />
        <activity android:name="com.thfw.mobileheart.activity.settings.SettingActivity" />
        <activity android:name="com.thfw.mobileheart.activity.study.StudyHomeActivity" />
        <activity android:name="com.thfw.mobileheart.activity.read.ReadHomeActivity" />
        <activity android:name="com.thfw.mobileheart.activity.video.VideoHomeActivity" />
        <activity android:name="com.thfw.mobileheart.activity.audio.AudioHomeActivity" />
        <activity android:name="com.thfw.mobileheart.activity.exercise.ExerciseActivity" />
        <activity android:name="com.thfw.mobileheart.activity.test.TestingActivity" />
        <activity android:name="com.thfw.mobileheart.activity.read.ReadEtcActivity" />
        <activity
            android:name="com.thfw.mobileheart.activity.audio.AudioPlayerActivity"
            android:theme="@style/AnimActivity" />
        <activity android:name="com.thfw.mobileheart.activity.StatusActivity" />
        <activity android:name="com.thfw.mobileheart.activity.login.ForgetPasswordActivity" />
        <activity
            android:name="com.thfw.mobileheart.activity.video.VideoPlayActivity"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:launchMode="singleInstance"
            android:screenOrientation="sensorPortrait" />
        <activity
            android:name="com.thfw.mobileheart.activity.talk.ChatActivity"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name="com.thfw.mobileheart.activity.talk.AskActivity"
            android:windowSoftInputMode="adjustResize" />
        <activity android:name="com.thfw.mobileheart.activity.ExoPlayerActivity" />
        <activity android:name="com.thfw.mobileheart.activity.TestActivity" />
        <activity
            android:name="com.thfw.mobileheart.activity.WebActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.thfw.mobileheart.activity.MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppThemeSplash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.thfw.mobileheart.activity.login.LoginActivity"
            android:screenOrientation="portrait" />
        <activity android:name="com.thfw.mobileheart.activity.AudioSuspendActivity" />

        <service android:name="com.thfw.base.net.download.DownLoadIntentService">
            <intent-filter>
                <action android:name="com.thfw.base.net.download.DownLoadIntentService" />
            </intent-filter>
        </service>
        <service android:name="com.thfw.mobileheart.AudioService" />
    </application>

</manifest>