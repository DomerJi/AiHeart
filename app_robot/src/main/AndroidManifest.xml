            android:name=".activitys.exercise.ExerciseActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".activitys.video.VideoPlayerActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".activitys.video.VideoHomeActivity"
